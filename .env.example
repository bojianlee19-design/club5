# Admin password for /admin (choose a strong one before deploying)
ADMIN_PASSWORD=change-me

# Vercel Blob (for images + events storage)
# Create in Vercel project settings -> Storage -> Blob, then add the token as an env var
BLOB_READ_WRITE_TOKEN=

# Optional: A public base URL for your site (used for OpenGraph)
NEXT_PUBLIC_SITE_URL=http://localhost:3000
